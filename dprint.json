{
  "typescript": {
    "lineWidth": 120,
    "indentWidth": 2,
    "useTabs": false,
    "semiColons": "always",
    "quoteStyle": "alwaysDouble",
    "quoteProps": "asNeeded",
    "newLineKind": "lf",
    "useBraces": "always",
    "trailingCommas": "onlyMultiLine",
    "bracePosition": "sameLine",
    "commentLine.forceSpaceAfterSlashes": true,
    "functionExpression.spaceAfterFunctionKeyword": true,
    "module.sortImportDeclarations": "maintain"
  },
  "excludes": [
    "**/node_modules",
    "**/*.d.ts"
  ],
  "includes": [
    "**/*.ts"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-latest.wasm"
  ]
}